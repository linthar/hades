<div class="application-login">
    <div class="header">
        <div class="logo"></div>
    </div>
    <div class="login-box">
        <div class="login-title">
            <h1>Acessar o Meu cadastro</h1>
        </div>
        <table>
            <tr>
                <td class="right"><span>Nome:</span></td>
                <td><div location="username"></div></td>
            </tr>
            <tr>
                <td class="right"><span>Senha:</span></td>
                <td><div location="password"></div></td>
            </tr>
            <tr>
                <td></td>
                <td><div location="errorLabel"></div></td>
            </tr>
            <tr>
                <td colspan="2" class="right">
                    <div location="loginButtonAndForgotPassword"></div>
                </td>
            </tr>
        </table>
    </div>
</div>